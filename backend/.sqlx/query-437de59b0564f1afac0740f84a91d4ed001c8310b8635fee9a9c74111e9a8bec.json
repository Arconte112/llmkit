  "query": "\n            SELECT\n                p.id,\n                p.key,\n                pv.system,\n                pv.user,\n                pv.model_id,\n                pv.max_tokens,\n                pv.temperature,\n                pv.json_mode,\n                m.name as model_name,\n                pr.name as provider_name,\n                pv.system_diff,\n                pv.user_diff,\n                pv.version_number,\n                pv.id as version_id,\n                pv.created_at,\n                pv.updated_at\n            FROM prompt p\n            JOIN prompt_version pv ON p.prompt_version_id = pv.id\n            JOIN model m ON pv.model_id = m.id\n            JOIN provider pr ON m.provider_id = pr.id\n            WHERE p.id = ?\n            ",
